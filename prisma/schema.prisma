// prisma/schema.prisma
datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Event {
  id String @id @default(cuid())
  type String
  title String 
  description String?
  dateTime DateTime
  location String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
